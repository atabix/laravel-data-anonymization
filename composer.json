    "laravel"
  ],
  "authors": [
    {
      "name": "Nekrasov Ilya",
      "email": "nekrasov.ilya90@gmail.com"
    },
    {
      "name": "Joris Kok",
      "email": "joris.kok@atabix.nl"
    }
